
import java.util.Scanner;


public class panamamusic {
 
    
public panamamusic crearpanamamusic(){
        int x = 0;
        primero = null;
        do{
            x = leerEntero();
            if(x != -1){
                primero = new Nodo(x,primero);
            }
        }while(x != -1);
        return this;
    }


public panamamusic insertarLista(int testigo, int entrada){
nodo nuevo, anterior;
anterior = buscarpanamamusic(testigo);
if (anterior != null){
    nuevo = new nodo(entrada);
    nuevo.enlace = anterior.enlace;
    anterior.enlace = nuevo;
}
return this;
}
}
   

